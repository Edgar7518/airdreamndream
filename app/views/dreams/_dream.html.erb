<div id="<%= dom_id dream %>">
  <p>
    <strong>Name:</strong>
    <%= dream.name %>
  </p>
   <p>
    <strong>Name:</strong>
    <%= dream.user.email%>
  </p>
  <p>
    <strong>Description:</strong>
    <%= dream.description %>
